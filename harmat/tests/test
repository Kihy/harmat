def A(a):
    if a:
        return 1
    return 0

def B(c,a, b):
    if a or c:
        if b:
            return 0.9
        return 0.1
    if b:
        return 0
    return 1

def C(a,c):
    if a:
        if c:
            return 0.9
        return 0.1
    if c:
        return 0
    return 1

def D(b,c,d):
    if b or c:
        if d:
            return 0.9
        return 0.1
    if d:
        return 0
    return 1

def posterior(a,b,c,d):
    return A(a)*C(a,c)*B(c,a,b)*D(b,c,d)

def enum():
    sum=0
    for a in [True,False]:
        for b in [True,False]:
            for c in [True,False]:
                for d in [True]:
                    print(posterior(a,b,c,d))
                    sum+=posterior(a,b,c,d)
    print(sum)
enum()

